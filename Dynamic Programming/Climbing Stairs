class Solution {
public:
    int climbStairs(int n) {
        vector<int> dp(n+1,-1);  // create a dp array
        return climbing(n,dp);

    }
    int climbing(int n,  vector<int>&dp)
    {
        if(n==0||n==1)return 1;    // using top-up approach
        if(dp[n]!=-1)return dp[n]; // memoisation
        return dp[n] = climbing(n-1,dp)+ climbing(n-2,dp);
    }
};
